msgid_plural "The last payday was {timespan_ago} and transferred {money_amount} between {n} users."
msgstr[0] "<uathúil gan úsáid (hash=fd1709b5b191a6dcb851e261d315f1e1)>"
msgstr[1] "Ba é {timespan_ago} an lá pá deiridh agus aistríodh {money_amount} idir {n} úsáideoir."
msgstr[2] "Ba é {timespan_ago} an lá pá deiridh agus aistríodh {money_amount} idir {n} úsáideoir."

#, python-brace-format
msgid "{n} participant gave money."
msgid_plural "{n} participants gave money."
msgstr[0] "Thug {n} rannpháirtí airgead."
msgstr[1] "Thug {n} rannpháirtí airgead."
msgstr[2] "Thug {n} rannpháirtí airgead."

#, python-brace-format
